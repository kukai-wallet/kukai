{
  "name": "kukai",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && angular-cli-ghpages --repo \"git@github.com:tezwallet/tezwallet.github.io.git\" --branch master",
    "alpha-deploy": "ng lint --fix && ng build --prod && angular-cli-ghpages --repo \"git@github.com:kukai-wallet/kukai-wallet.github.io.git\" --branch master",
    "pre-build-electron": "ng lint --fix && ng build --prod --base-href . && copy src\\electron\\* dist && mkdir dist\\src && move dist\\assets dist\\src\\assets",
    "build-electron-win": "electron-packager dist --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Kukai --version-string.FileDescription=Kukai --version-string.ProductName=\"Kukai\"",
    "build-electron-mac": "electron-packager dist --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "build-electron-linux": "electron-packager dist --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "build-electron": "npm run pre-build-electron && npm run build-electron-win",
    "run-electron": "npm run build-electron && electron dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.1",
    "@types/crypto-js": "^3.1.39",
    "aes-js": "^3.1.1",
    "angular-cli-ghpages": "^0.5.2",
    "bip39": "^2.4.0",
    "bootstrap": "^4.0.0",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.1",
    "buffer": "^5.0.7",
    "codecov": "^2.3.1",
    "copy-to-clipboard": "^3.0.8",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "file-saver": "^1.3.8",
    "jquery": "^3.3.1",
    "libsodium-wrappers": "^0.5.4",
    "ngx-bootstrap": "^2.0.2",
    "npm": "^5.10.0",
    "pbkdf2": "^3.0.14",
    "qrcode": "^1.2.0",
    "randomatic": "^3.0.0",
    "rxjs": "^5.5.6",
    "secure-password": "^3.0.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.8",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^2.0.4",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
